<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PrimaryController">
   <children>
      <Label text="Primary View" />
      <Label text="Input n:" />
      <TextField fx:id="inputN" maxWidth="200"/>
      <Label text="Calculate p and q" />
      <Label text="p is:"/>
      <Text fx:id="textP"/>
      <Label text="q is:"/>
      <Text fx:id="textQ"/>
      <Label text="e is:"/>
      <Text fx:id="resultE"/>
      <Button fx:id="calculatePQ" text="Calculate P and Q" onAction="#primeFactorization"/>
      <Button fx:id="calculateE" text="Calculate E" onAction="#relativelyPrime"/>
      <Button fx:id="primaryButton" text="Switch to Secondary View" onAction="#switchToSecondary"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
